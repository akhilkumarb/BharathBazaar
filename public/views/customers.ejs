<!DOCTYPE html>
<!-- Coding by CodingNepal | www.codingnepalweb.com -->
<html lang="en" dir="ltr">
  <head>
    <meta charset="UTF-8">
    <title> Responsiive Admin Dashboard | CodingLab </title>
    <!-- Boxicons CDN Link -->
    <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
     <link rel="stylesheet" href="/css/customers.css">
   </head>
<body>
    <div class="sidebar">
        <div class="logo-details">
            <span class="logo_name"><span style="color:rgb(217, 153, 84);">B</span><span style="color:rgb(80, 207, 80)">B</span></span>
        </div>
        <ul class="nav-links">
            <li>
                <a href="/admin/dashboard" >
                    <i class='bx bx-grid-alt'></i>
                    <span class="links_name">Dashboard</span>
                </a>
            </li>
            <li>
                <a href="/admin/customers" class="active">
                    <i class="fa-solid fa-user"></i>
                    <span class="links_name">Customers</span>
                </a>
            </li>
            <li>
                <a href="/admin/orders">
                    <i class="fa-solid fa-truck-fast"></i>
                    <span class="links_name">Orders</span>
                </a>
            </li>
            <li>
                <a href="/admin/favourites">
                    <i class="fa-solid fa-heart"></i>
                    <span class="links_name">Favourites</span>
                </a>
            </li>
            <li>
                <a href="/admin/products">
                    <i class='bx bx-box'></i>
                    <span class="links_name">Products</span>
                </a>
            </li>
            <li> 
                <a href="/admin/total-orders">
                    <i class="fa-solid fa-sack-dollar"></i>
                    <span class="links_name">Total orders</span>
                </a>
            </li>
            <li>
                <a href="/admin/addnewproduct" >
                    <i class="fa-solid fa-circle-plus"></i>
                    <span class="links_name">Add Product</span>
                </a>
            </li>
    
            <li>
                <a href="/admin/update">
                    <i class="fa-regular fa-circle-check"></i>
                    <span class="links_name">Update</span>
                </a>
            </li>
            <li>
                <a href="/admin/delete">
                    <i class="fa-solid fa-trash-can"></i>
                    <span class="links_name">Delete</span>
                </a>
            </li>
            
    
            <li class="log_out">
                <a href="/login">
                    <i class='bx bx-log-out'></i>
                    <span class="links_name">logout</span>
                </a>
            </li>
        </ul>
        </div>
  <section class="home-section">
    <nav>
      <div class="sidebar-button">
        <i class='bx bx-menu sidebarBtn'></i>
        <span class="dashboard">Customers</span>
      </div>

      <div class="profile-details">
        <img src="/images/k2.jpg" alt="">
        <span class="admin_name">Ganta Venkata Kousik</span>

      </div>
    </nav>

    <div class="home-content">
        <div class="overview-boxes">
            <% u.forEach(user => { %>
          <div class="box">
            <div class="right-side">
              <div class="number"><span> UserName : </span><%= user.name %></div>
              
              <div class="number"><span> Email : </span><%= user.email %></div>
              
              <div class="number"><span>Password : </span><%= user.password %></div>
              
            </div>
          </div>
          <% }); %>
        </div>
    </div>
  </section>

<script src="/css/admin.js"></script>

</body>
</html>