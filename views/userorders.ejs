<!DOCTYPE html>
<!-- Coding By CodingNepal - codingnepalweb.com -->
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">

        <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <title>User Dashboard Panel</title>
    <link rel="stylesheet" href="/css/products.css">

</head>
<body>
    <div class="sidebar">
        <div class="logo-details">
          <span class="logo_name"><span style="color:rgb(217, 153, 84);">B</span><span style="color:rgb(80, 207, 80)">B</span></span>
        </div>
        <ul class="nav-links">
          <li>
              <a href="/user/dashboard?userid=<%= encodeURIComponent(user._id) %>" >
                  <i class='bx bx-grid-alt'></i>
                  <span class="links_name">Dashboard</span>
              </a>
          </li>

          <li>
              <a href="/user/orders?userid=<%= encodeURIComponent(user._id) %>" class="active">
                  <i class="fa-solid fa-truck-fast"></i>
                  <span class="links_name">My Orders</span>
              </a>
          </li>

          <li>
              <a href="/user/addproduct?userid=<%= encodeURIComponent(user._id) %>" > 
                <i class="fa-solid fa-industry"></i>
                  <span class="links_name">Produce Item</span>
              </a>
          </li>

          <li>
            <a href="/user/producer?userid=<%= encodeURIComponent(user._id) %>" > 
              <i class="fa-solid fa-signal"></i>
                <span class="links_name">Product Stats</span>
            </a>
        </li>

        <li>
          <a href="/user/appointments?userid=<%= encodeURIComponent(user._id) %>">
              <i class='bx bx-grid-alt'></i>
              <span class="links_name">appointments</span>
          </a>
      </li>

          <li>
            <a href="/producer/notifications?userid=<%= encodeURIComponent(user._id) %>" > 
                <i class="fa-solid fa-bell"></i>
                <span class="links_name">Notifications</span>
            </a>
        </li>

          <li>
              <a href="/user/settings?userid=<%= encodeURIComponent(user._id) %>">
                  <i class="fa-solid fa-gear"></i>
                  <span class="links_name">Settings</span>
              </a>
          </li>
              
          <li class="log_out">
              <a href="/login">
                  <i class='bx bx-log-out'></i>
                  <span class="links_name">logout</span>
              </a>
          </li>
      </ul>
        </div>
        <section class="home-section">
            <nav>
              <div class="sidebar-button">
                <i class='bx bx-menu sidebarBtn'></i>
                <span class="dashboard">My Orders</span>
              </div>
        
              <div class="profile-details">
                
                <span class="admin_name"><%= user.name %></span>
              </div>
            </nav>
            <div class="py-5">
                <div class="container-fluid">
                  
                  <div class="row">
                    <div class="col-md-12">
          
                      <div class="bootstrap-tabs product-tabs">
                          <div class="col">
                              <% products.forEach(product => { %>
                                  
                              <div class="product-card product-item">
                                
                                
                                <div class="main-images">
                                  
                                  <img src= <%= product.imgUrl %> class="tab-image">
                                
                                </div>
                                <div class="shoe-details">
                                  <div class="shoe_name"><%= product.name.toLowerCase().split(' ').map(word => word.charAt(0).toUpperCase() + word.slice(1)).join(' ') %>
                                  </div>
                                  
     
                                </div>
                                <div class="color-price">
                                  
                                  <div class="price">
                                    <span class="price price_num">$<%= product.price %></span>
                                  </div>
                                </div>

                              </div>
                              
                            <% }); %>
                          </div>
          
          </section>
        </div>
    




</div>
</body>
</html>